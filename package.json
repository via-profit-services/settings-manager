{
    "name": "@via-profit-services/settings-manager",
    "support": "https://via-profit.ru",
    "version": "1.1.1",
    "description": "Via Profit Settings Manager Schema",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 12.8.3",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "via-profit",
        "settings"
    ],
    "scripts": {
        "postinstall": "node ./scripts/postinstall.js",
        "dist": "yarn lint && yarn build:dist",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "build:migrations": "webpack --config ./webpack/webpack-config-migrations.ts",
        "start": "yarn build:dev --watch",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "migrate:rollback": "yarn knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:latest": "yarn knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:up": "yarn knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "yarn knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:make": "yarn knex migrate:make --migrations-directory ./src/database/migrations -x ts",
        "seed": "yarn knex seed:run --knexfile ./.knex/knexfile.js",
        "postversion": "git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/settings-manager.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vaily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "peerDependencies": {
        "@via-profit-services/core": "^1.0.0",
        "@via-profit-services/knex": "^1.0.0",
        "dataloader": "*",
        "graphql": "*",
        "moment": "*",
        "moment-timezone": "*",
        "uuid": "*"
    },
    "devDependencies": {
        "@graphql-tools/schema": "^7.1.2",
        "@graphql-tools/utils": "^7.1.6",
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@types/express": "^4.17.9",
        "@types/express-serve-static-core": "^4.17.2",
        "@types/ioredis": "^4.16.0",
        "@types/jest": "^24.0.23",
        "@types/moment-timezone": "^0.5.12",
        "@types/node": "^14.14.14",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.2",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@via-profit-services/core": "^1.0.5",
        "@via-profit-services/eslint-config": "^0.1.11",
        "@via-profit-services/knex": "^1.0.4",
        "@zerollup/ts-transform-paths": "^1.7.11",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^6.0.3",
        "dataloader": "^2.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "express": "^4.17.1",
        "filemanager-webpack-plugin": "^2.0.5",
        "graphql": "^15.4.0",
        "graphql-import-node": "^0.0.4",
        "graphql-tag": "^2.11.0",
        "ioredis": "^4.19.4",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "knex": "^0.21.14",
        "libphonenumber-js": "^1.9.7",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "nodemon": "^2.0.2",
        "nodemon-webpack-plugin": "^4.2.2",
        "pg": "^8.5.1",
        "prettier": "^2.1.2",
        "raw-loader": "^4.0.2",
        "remove-files-webpack-plugin": "^1.2.2",
        "supertest": "^4.0.2",
        "supports-color": "^8.1.0",
        "terser-webpack-plugin": "^2.3.5",
        "ts-jest": "^24.2.0",
        "ts-loader": "^8.0.12",
        "ts-node": "^9.1.1",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^4.0.5",
        "uuid": "^7.0.2",
        "webpack": "^5.11.0",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.2",
        "webpack-node-externals": "^1.7.2",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    }
}
